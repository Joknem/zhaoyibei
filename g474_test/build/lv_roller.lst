 124:lvgl/src/widgets/lv_roller.c ****         uint8_t i;
 125:lvgl/src/widgets/lv_roller.c ****         for(i = 0; i < LV_ROLLER_INF_PAGES; i++) {
 126:lvgl/src/widgets/lv_roller.c ****             strcpy(&opt_extra[opt_len * i], options);
