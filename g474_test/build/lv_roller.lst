 155:lvgl/src/widgets/lv_roller.c **** 
 156:lvgl/src/widgets/lv_roller.c ****     /*Set the value even if it's the same as the current value because
 157:lvgl/src/widgets/lv_roller.c ****      *if moving to the next option with an animation which was just deleted in the PRESS Call the a
 158:lvgl/src/widgets/lv_roller.c ****      *nothing will continue the animation.*/
 159:lvgl/src/widgets/lv_roller.c **** 
 160:lvgl/src/widgets/lv_roller.c ****     lv_roller_t * roller = (lv_roller_t *)obj;
 161:lvgl/src/widgets/lv_roller.c **** 
 162:lvgl/src/widgets/lv_roller.c ****     /*In infinite mode interpret the new ID relative to the currently visible "page"*/
 163:lvgl/src/widgets/lv_roller.c ****     if(roller->mode == LV_ROLLER_MODE_INFINITE) {
 164:lvgl/src/widgets/lv_roller.c ****         uint32_t real_option_cnt = roller->option_cnt / LV_ROLLER_INF_PAGES;
 165:lvgl/src/widgets/lv_roller.c ****         uint16_t current_page = roller->sel_opt_id / real_option_cnt;
 166:lvgl/src/widgets/lv_roller.c ****         /*Set by the user to e.g. 0, 1, 2, 3...
 167:lvgl/src/widgets/lv_roller.c ****          *Upscale the value to the current page*/
 168:lvgl/src/widgets/lv_roller.c ****         if(sel_opt < real_option_cnt) {
 169:lvgl/src/widgets/lv_roller.c ****             uint16_t act_opt = roller->sel_opt_id - current_page * real_option_cnt;
 170:lvgl/src/widgets/lv_roller.c ****             int32_t sel_opt_signed = sel_opt;
 171:lvgl/src/widgets/lv_roller.c ****             /*Huge jump? Probably from last to first or first to last option.*/
 172:lvgl/src/widgets/lv_roller.c ****             if(LV_ABS((int16_t)act_opt - sel_opt) > real_option_cnt / 2) {
 173:lvgl/src/widgets/lv_roller.c ****                 if(act_opt > sel_opt) sel_opt_signed += real_option_cnt;
 174:lvgl/src/widgets/lv_roller.c ****                 else sel_opt_signed -= real_option_cnt;
 175:lvgl/src/widgets/lv_roller.c ****             }
 176:lvgl/src/widgets/lv_roller.c ****             sel_opt = sel_opt_signed + real_option_cnt * current_page;
 177:lvgl/src/widgets/lv_roller.c ****         }
 178:lvgl/src/widgets/lv_roller.c ****     }
 179:lvgl/src/widgets/lv_roller.c **** 
 180:lvgl/src/widgets/lv_roller.c ****     roller->sel_opt_id     = sel_opt < roller->option_cnt ? sel_opt : roller->option_cnt - 1;
 181:lvgl/src/widgets/lv_roller.c ****     roller->sel_opt_id_ori = roller->sel_opt_id;
 182:lvgl/src/widgets/lv_roller.c **** 
 183:lvgl/src/widgets/lv_roller.c ****     refr_position(obj, anim);
 184:lvgl/src/widgets/lv_roller.c **** }
